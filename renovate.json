{
  "extends": [
    "github>nuxt/renovate-config-nuxt"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "baseBranches": ["v1", "v3"],
  "packageRules": [{
    "matchBaseBranches": ["v3"],
    "labels": ["v3"]
  }, {
    "matchDepTypes": ["peerDependencies"],
    "enabled": false
  }, {
    "matchDepTypes": ["resolutions"],
    "enabled": false
  }],
  "postUpdateOptions": ["pnpmDedupe"]
}
